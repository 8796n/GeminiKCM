type OVERLAY

map key 54  META_LEFT
map key 51  SLASH

### ROW 1

key 1 {
    label:                              '1'
    base:                               '1'
    shift:                              '!'
    fn:                                 '|'
}

key 2 {
    label:                              '2'
    base:                               '2'
    shift:                              '"'
    fn:                                 '`'
}

key 3 {
    label:                              '3'
    base:                               '3'
    shift:                              '#'
    fn:                                 '\\'
}

key 4 {
    label:                              '4'
    base:                               '4'
    shift:                              '$'
    fn:                                 '\u00A5'
}

key 5 {
    label:                              '5'
    base:                               '5'
    shift:                              '%'
    fn:                                 '<'
}

key 6 {
    label:                              '6'
    base:                               '6'
    shift:                              '&'
    fn:                                 '>'
    alt:                                replace F6
}

key 7 {
    label:                              '7'
    base:                               '7'
    shift:                              '\''
    fn:                                 '['
    fn+shift:                           '{'
    alt:                                replace F7
}

key 8 {
    label:                              '8'
    base:                               '8'
    shift:                              '('
    fn:                                 ']'
    fn+shift:                           '}'
    alt:                                replace F8
}

key 9 {
    label:                              '9'
    base:                               '9'
    shift:                              ')'
    alt:                                replace F9
}

key 0 {
    label:                              '0'
    base:                               '0'
    shift:                              '~'
    alt:                                replace F10
}

key DEL {
    base:                               fallback DEL
    shift:                              fallback FORWARD_DEL
}

