### ROW 5

#key CTRL_LEFT {
#}

#key FUNCTION {
#}

key ALT_LEFT {
    base:                               replace ALT_LEFT
    fn:                                 replace EISU
}

key SPACE {
    label:                              ' '
    base:                               ' '
    alt:                                fallback    SEARCH
    ctrl:                               fallback    LANGUAGE_SWITCH
}

key SLASH {
    label:                              ','
    base:                               ','
    shift:                              '-'
    fn:                                 replace KANA
}

key DPAD_LEFT {
   base:                                fallback DPAD_LEFT
   fn:                                  replace MOVE_HOME
   alt:                                 replace BACK
}

key DPAD_DOWN {
   base:                                fallback DPAD_DOWN
   fn:                                  replace PAGE_DOWN
   alt:                                 replace HOME
}

key DPAD_RIGHT {
   base:                                fallback DPAD_RIGHT
   fn:                                  replace MOVE_END
   alt:                                 replace FORWARD
}