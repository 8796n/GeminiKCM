type OVERLAY

### ROW 1

key 1 {
    label:                              '1'
    base:                               '1'
    shift:                              '!'
    fn:                                 '|'
}

key 2 {
    label:                              '2'
    base:                               '2'
    shift:                              '"'
    fn:                                 '`'
}

